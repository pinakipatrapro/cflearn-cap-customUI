{
    "welcomeFile": "app/index.html",
    "authenticationMethod": "route",
    "logout": {
        "logoutEndpoint": "/do/logout"
    },
    "routes": [
        {
            "source": "/browse/(.*)$",
            "target": "/browse/$1",
            "authenticationType": "xsuaa",
            "destination": "srv_api",
            "csrfProtection": true
        },
        {
            "source": "/v2/(.*)$",
            "target": "/v2/$1",
            "authenticationType": "xsuaa",
            "destination": "srv_api",
            "csrfProtection": true
        },
        {
            "source": "^/app/(.*)$",
            "target": "$1",
            "localDir": "dist"
        },
        {
            "source": "^/appdebug/(.*)$",
            "target": "$1",
            "localDir": "webapp"
        }
    ]
}